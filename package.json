{
  "name": "echaloasuerte",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NODE_OPTIONS='--inspect' next",
    "start": "next start -p 80",
    "start:integration": "REACT_APP_ENV=test npm run build && REACT_APP_ENV=test HTTPS=false npm run start -- -p 3005",
    "start:integration:dev": "REACT_APP_ENV=test HTTPS=false npm run dev -- -p 3005",
    "build": "next build",
    "export": "next export",
    "test": "jest --watch --testPathIgnorePatterns config/test.js",
    "test:ci": "jest --ci --testPathIgnorePatterns config/test.js",
    "cypress": "TZ=America/New_York cypress open",
    "cypress:ci": "TZ=America/New_York cypress run",
    "test:integration:dev": "npm run cypress & npm run start:integration:dev",
    "test:integration": "npm run cypress & npm run start:integration",
    "test:integration:ci": "npm run start:integration & wait-on http://localhost:3005 && npm run cypress:ci",
    "lint:scss": "stylelint 'components/**/*.module.scss' --syntax scss",
    "lint:scss:fix": "stylelint  'components/**/*.module.scss' --syntax scss --fix",
    "lint:js": "eslint . --ignore-path .gitignore --ext .js,.jsx",
    "lint:js:fix": "npm run lint:js -- --fix",
    "lint": "npm run lint:js && npm run lint:scss",
    "analyze": "ANALYZE=true npm run build"
  },
  "dependencies": {
