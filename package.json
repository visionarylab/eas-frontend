  "dependencies": {
    "moment": "^2.22.2",
    "react-event-listener": "^0.6.1",
    "react-helmet": "^5.2.0",
    "react-router-dom": "^4.2.2"
  },
  "devDependencies": {
    "@material-ui/core": "^1.1.0",
    "@material-ui/icons": "^1.1.0",
    "backpack-react-scripts": "4.0.8",
    "classnames": "^2.2.5",
    "cypress": "^3.1.0",
    "echaloasuerte-js-sdk": "0.0.4",
    "enzyme": "^3.5.1",
    "enzyme-adapter-react-16": "^1.4.0",
    "enzyme-to-json": "^3.3.4",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-cypress": "^2.0.1",
    "eslint-plugin-prettier": "^2.6.0",
    "i18next": "^11.2.3",
    "i18next-browser-languagedetector": "^2.2.0",
    "material-ui-pickers": "^1.0.0-rc.12",
    "prettier": "^1.14.2",
    "prop-types": "^15.6.0",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-ga": "^2.5.3",
    "react-i18next": "^7.6.0",
    "react-router-prop-types": "^1.0.3",
    "react-tag-input": "^5.0.2",
    "react-validation": "^3.0.7",
    "superagent": "^3.8.3"
  },
  "scripts": {
    "start": "REACT_APP_ENV=local react-scripts start",
    "start:test": "REACT_APP_ENV=test react-scripts start",
    "build": "REACT_APP_ENV=production react-scripts build",
    "build:dev": "REACT_APP_ENV=development react-scripts build",
    "test": "react-scripts test --env=jsdom --testPathIgnorePatterns src/config/test.js",
    "lint:scss": "stylelint 'src/**/*.scss' --syntax scss",
    "lint:scss:fix": "stylelint  'src/**/*.scss' --syntax scss --fix",
    "lint:js": "eslint . --ignore-path .gitignore --ext .js,.jsx",
    "lint:js:fix": "npm run lint:js -- --fix",
    "lint": "npm run lint:js && npm run lint:scss",
    "test:integration:ci": "cypress run",
    "test:integration": "cypress open",
    "eject": "react-scripts eject"
  },
  "backpack-react-scripts": {
    "babelIncludePrefixes": [
      "echaloasuerte-js-sdk"
    ]
  }
